<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<PackageTags>freesql;freekit;localization;</PackageTags>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<Description>.net core localization FreeSql</Description>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="FreeSql" Version="$(DefaultFreeeSqlVersion)" />
		<PackageReference Include="Microsoft.Extensions.Localization" Version="$(DefaultAspNetCoreVersion)" />
	</ItemGroup>
</Project>
